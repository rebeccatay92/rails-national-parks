<%= render 'navbar' %>

<h1>Edit listing</h1>

<%= form_for @park, url: parks_update_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :picture %><br>
    <%= f.text_area :picture %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
